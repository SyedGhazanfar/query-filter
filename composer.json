        }
    },
   
 "require": {
        "php": "^7.3 || ^8.0 || 8.1",
        "illuminate/database": "^9.35",
        "illuminate/support": "^9.35"
    },

"require-dev": {
     "nunomaduro/larastan": "2.2.1",
        "orchestra/canvas": "7.x-dev",
        "orchestra/testbench": "^7.10"
},
    "config": {
        "sort-packages": true
    }
}
