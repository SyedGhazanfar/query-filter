{
    "name": "laravie/query-filter",
    "description": "Database/Eloquent Query Builder filters for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\QueryFilter\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\QueryFilter\\Tests\\" : "tests/"
        }
    },
   
 "require": {
        "php": "^7.3 || ^8.0 || 8.1",
        "illuminate/database": "10.x-dev",
        "illuminate/support": "10.x-dev"
    },
    "require-dev": {
        "nunomaduro/larastan": "3.0.x-dev",
        "orchestra/canvas": "8.0.x-dev",
        "orchestra/testbench": "8.0.x-dev"
    },

    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
